{
  "name": "zeehours-pub",
  "version": "1.0.0",
  "description": "MUD client for zeehours.net with web interface",
  "main": "dist/cli/index.js",
  "scripts": {
    "build": "tsc --build tsconfig.server.json",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/server/index.js",
    "web": "ts-node src/server/index.ts",
    "web:build": "npm run build && node dist/server/index.js",
    "query": "ts-node src/query-events.ts",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix"
  },
  "keywords": [
    "mud",
    "telnet",
    "client",
    "web"
  ],
  "author": "awwaiid",
  "license": "MIT",
  "dependencies": {
    "@next/font": "^14.2.15",
    "ansi-to-html": "^0.7.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-session": "^1.18.1",
    "next": "^15.3.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "readline": "^1.3.0",
    "sqlite3": "^5.1.7",
    "telnet-client": "^2.2.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.12.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sqlite3": "^3.1.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
